<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:app2="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="landingPageVM"
            type="com.example.eduempoweryd.login.landingpage.LandingPageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameLandingpage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImageThree"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY"
            android:src="@drawable/img_image3"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image3"/>
        <LinearLayout android:id="@+id/linearColumnsalytwentysix"
            android:layout_gravity="top|center"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_246pxv"
            android:layout_marginEnd="@dimen/_71pxh"
            android:layout_marginStart="@dimen/_71pxh"
            android:layout_marginTop="@dimen/_246pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <FrameLayout android:id="@+id/frameStacksalytwentysix"
                android:layout_height="@dimen/_220pxv"
                android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_220pxh">
                <ImageView android:id="@+id/imageSalyTwentySix"
                    android:layout_gravity="top|start"
                    android:layout_height="@dimen/_49pxh"
                    android:layout_marginBottom="@dimen/_25pxv"
                    android:layout_marginEnd="@dimen/_21pxh"
                    android:layout_marginStart="@dimen/_21pxh"
                    android:layout_marginTop="@dimen/_25pxv"
                    android:layout_width="@dimen/_39pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_saly26"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_saly26"/>
                <ImageView android:id="@+id/imageSalyEleven"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_220pxh"
                    android:layout_width="match_parent"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_saly11"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_saly11"/>
            </FrameLayout>

            <TextView
                android:ellipsize="end"
                android:gravity="start"
                android:id="@+id/txtEDUEMPOWER"
                android:textSize="35sp"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_1pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_eduempower"
                style="@style/txt"
                tools:text="@string/lbl_eduempower"/>
        </LinearLayout>
    </FrameLayout>

</layout>